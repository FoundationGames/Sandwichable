{
  "schemaVersion": 1,
  "id": "sandwichable",
  "version": "${version}",

  "name": "Sandwichable",
  "description": "Sandwiches. Go nuts.",
  "authors": [
    "FoundationGames"
  ],
  "contact": {
    "homepage": "https://github.com/FoundationGames/Sandwichable",
    "sources": "https://github.com/FoundationGames/Sandwichable"
  },

  "license": "MIT License",
  "icon": "assets/sandwichable/icon.png",

