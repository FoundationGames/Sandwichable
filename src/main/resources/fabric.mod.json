    ],
	"emi": [
      "io.github.foundationgames.sandwichable.plugin.emi.SandwichableEMI"
    ],
    "modmenu": [
      "io.github.foundationgames.sandwichable.plugin.SandwichableModMenu"
    ],
    "appleskin": [
      "io.github.foundationgames.sandwichable.plugin.SandwichableAppleSkin"
    ]
  },
  "mixins": [
	"sandwichable.mixins.json"
  ],
  "accessWidener": "sandwichable.accesswidener",
  "depends": {
    "fabricloader": ">=0.11.0",
